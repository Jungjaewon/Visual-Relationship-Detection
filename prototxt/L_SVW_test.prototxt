input: "ims"
input_shape {
	dim: 1
	dim: 3
	dim: 224
	dim: 224
}
input: "spatial"
input_shape {
	dim: 1
	dim: 7
}
input: "pair_vector"
input_shape {
	dim: 1
	dim: 600
}
input: "factor"
input_shape{
	dim: 1
	dim: 70
}
# Visual module!!!
layer {
	name: "conv1_1"
	type: "Convolution"
	bottom: "ims"
	top: "conv1_1"
	param {
		lr_mult: 1
		decay_mult: 1
	} 
	param {
		lr_mult: 1
		decay_mult: 1
	}
	convolution_param {
		num_output: 64
		pad: 1
		kernel_size: 3
	}
}
layer {
	name: "relu1_1"
	type: "ReLU"
	bottom: "conv1_1"
	top: "rconv1_1"
}
layer {
	name: "conv1_2"
	type: "Convolution"
	bottom: "rconv1_1"
	top: "conv1_2"
	param {
